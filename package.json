{
  "name": "@mucan54/porterman",
  "version": "1.4.0",
  "description": "Zero-config HTTPS tunnels for your local ports. Powered by Cloudflare.",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "porterman": "./bin/porterman.mjs"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "tunnel",
    "ngrok",
    "https",
    "cloudflare",
    "cloudflared",
    "expose",
    "localhost",
    "reverse-proxy",
    "trycloudflare"
  ],
  "author": "mucan54",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mucan54/porterman.git"
  },
  "bugs": {
    "url": "https://github.com/mucan54/porterman/issues"
  },
  "homepage": "https://github.com/mucan54/porterman#readme",
  "license": "MIT",
  "dependencies": {
    "cac": "^6.7.14",
    "cloudflared": "^0.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
